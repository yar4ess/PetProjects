<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="618.0" prefWidth="1002.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.TableController">
   <children>
      <Button fx:id="BackToMenuButton" layoutX="58.0" layoutY="32.0" mnemonicParsing="false" onAction="#switchToMain" prefHeight="44.0" prefWidth="116.0" style="-fx-background-color: gray;" text="%back_to_menu" textFill="WHITE">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <TableView fx:id="movieTable" layoutX="58.0" layoutY="154.0" prefHeight="338.0" prefWidth="887.0">
         <columns>
            <TableColumn fx:id="idColumn" text="ID" />
            <TableColumn fx:id="nameColumn" text="Name" />
            <TableColumn fx:id="coordinatesX" text="CoordX" />
            <TableColumn fx:id="coordinatesY" text="CoordY" />
            <TableColumn fx:id="oscarsCountColumn" text="OscarsCount" />
            <TableColumn fx:id="budgetColumn" text="Budget" />
            <TableColumn fx:id="taglineColumn" text="Tagline" />
            <TableColumn fx:id="genreColumn" text="Genre" />
            <TableColumn fx:id="screenWriterNameColumn" text="SWName" />
            <TableColumn fx:id="screenWriterBirth" text="SWBirth" />
            <TableColumn fx:id="locationX" text="locX" />
            <TableColumn fx:id="locationY" text="locY" />
            <TableColumn fx:id="locationZ" text="locZ" />
            <TableColumn fx:id="locationName" minWidth="0.0" prefWidth="0.0" text="locName" />
         </columns>
      </TableView>
      <Button layoutX="58.0" layoutY="97.0" mnemonicParsing="false" onAction="#handleRefreshTableButtonAction" prefHeight="44.0" prefWidth="116.0" style="-fx-background-color: gray;" text="Refresh" textFill="WHITE">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
