<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="10.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.ConfirmationDialogController">
    <children>
        <Button fx:id="editButton" mnemonicParsing="false" onAction="#handleEdit" text="%edit" />
        <Button fx:id="deleteButton" mnemonicParsing="false" onAction="#handleDelete" text="%delete" />
    </children>
</VBox>
